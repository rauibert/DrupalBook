{
    "name": "drupal/ckeditor_videodetector",
    "type": "drupal-module",
    "description": "This plugin allows you to insert videos from Youtube, Vimeo or Dailymotion only pasting an URL or embed code. The inserted videos will catch the full CKEditor width and will be fully responsive.",
    "keywords": [
        "Drupal",
        "Ckeditor",
        "Video Detector"
    ],
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "George Anderson (geoanders)",
            "homepage": "https://www.drupal.org/u/geoanders",
            "role": "Maintainer"
        }
    ],
    "homepage": "https://www.drupal.org/project/ckeditor_videodetector",
    "minimum-stability": "dev",
    "support": {
        "issues": "https://drupal.org/project/issues/ckeditor_videodetector",
        "source": "https://git.drupalcode.org/project/ckeditor_videodetector"
    },
    "require": {
        "drupal/core": "^8 || ^9 || ^10"
    }
}
